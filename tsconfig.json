{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "src",
    "slate.config.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["src/*"],
      "~@/*": ["./*"]
    },
    "jsxImportSource": "react",
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
